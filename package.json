{
  "name": "farmify-backend",
  "version": "1.0.0",
  "description": "farm data APIs",
  "main": "index.js",
  "scripts": {
    "tsc": "cross-env NODE_ENV=production tsc",
    "copyCSVFilesToBuild": "copyfiles -f ./data/initialFarms/*.csv ./build/data/initialFarms && copyfiles -f ./data/newFarms/*.csv ./build/data/newFarms",
    "postinstall": "npm run tsc && npm run copyCSVFilesToBuild",
    "start": "cross-env NODE_ENV=production node build/src/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --files src/index.ts",
    "lint": "eslint --ext .ts .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ambeche/farmify-backend.git"
  },
  "keywords": [
    "TypeScript",
    "nodejs",
    "express"
  ],
  "author": "Tamanji Che",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ambeche/farmify-backend/issues"
  },
  "homepage": "https://github.com/ambeche/farmify-backend#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.8",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@types/umzug": "^2.3.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "formidable": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.1",
    "sequelize": "^6.12.5",
    "umzug": "^3.0.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": "16.x"
  }
}
